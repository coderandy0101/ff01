	Func resigntooutpost($aisdead)
		out("Resigning")
		resign()
		Do
			Sleep(100)
		Until getisdead($myptr)
		If $aisdead Then
			rndsleep(4000)
		Else
			rndsleep(1500)
		EndIf
		returntooutpost()
		waitmaploading($mapid_anjeka)
	EndFunc

