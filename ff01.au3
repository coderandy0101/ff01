		If $lmodmemory = 0 Then
			$mmemory = DllCall($mkernelhandle, "ptr", "VirtualAllocEx", "handle", $mgwprochandle, "ptr", 0, "ulong_ptr", $masmsize, "dword", 4096, "dword", 64)
			$mmemory = $mmemory[0]
			memorywrite(memoryread($mbase), $mmemory)
		Else
