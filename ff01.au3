	Func _travelgh()
		Local $larray_gh[16] = [4, 5, 6, 52, 176, 177, 178, 179, 275, 276, 359, 360, 529, 530, 537, 538]
		Local $lmapid = getmapid()
		If _arraysearch($larray_gh, $lmapid) <> -1 Then Return
		travelgh()
