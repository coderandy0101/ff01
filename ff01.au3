	EndIf
	If _winapi_inprocess($hwnd, $__g_hsblastwnd) Then
		_sendmessage($hwnd, $sb_gettextw, $ipart, $tbuffer, 0, "wparam", "struct*")
	Else
		Local $tmemmap
