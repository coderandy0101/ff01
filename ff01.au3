	Func bin64todec($abinary)
		Local $lreturn = 0
		For $i = 1 To StringLen($abinary)
			If StringMid($abinary, $i, 1) == 1 Then $lreturn += 2 ^ ($i - 1)
		Next
		Return $lreturn
	EndFunc

