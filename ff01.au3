	If $aslot = 0 Then
		$lslot = 1
	Else
		$lslot = $aslot
	EndIf
	$abagnr = 0
	$aslot = 0
	For $bag = $lbagnr To 16
		$lbagptr = getbagptr($bag)
		If $lbagptr = 0 Then Return 0
