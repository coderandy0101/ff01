	Func disconnected()
		ControlSend(getwindowhandle(), "", "", "{Enter}")
		Local $lcheck = False
		Local $ldeadlock = TimerInit()
		Do
			Sleep(20)
			$lcheck = getmaploading() <> 2 AND getagentexists(-2)
		Until $lcheck OR TimerDiff($ldeadlock) > 60000
		If $lcheck = False Then
			ControlSend(getwindowhandle(), "", "", "{Enter}")
			$ldeadlock = TimerInit()
			Do
				Sleep(20)
				$lcheck = getmaploading() <> 2 AND getagentexists(-2)
			Until $lcheck OR TimerDiff($ldeadlock) > 60000
			If $lcheck = False Then
				Exit 1
			EndIf
		EndIf
		Sleep(5000)
	EndFunc

