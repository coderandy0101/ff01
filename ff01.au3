		Sleep(100)
		displayall(False)
		For $i = 1 To getmaxagents()
			$laddress = $mstringlogbase + 256 * $i
			$lname = memoryread($laddress, "wchar [128]")
			If $lname = "" Then ContinueLoop
			$lname = StringRegExpReplace($lname, "[<]{1}([^>]+)[>]{1}", "")
			If StringInStr($lname, $aname) <> 0 Then Return $i
